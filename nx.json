{
  "npmScope": "qovery",
  "affected": {
    "defaultBase": "staging"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "ZDhiYzMyN2QtNTg2ZS00ZWU0LWJhMjQtYWY4Y2I4Y2QwMTE5fHJlYWQtd3JpdGU=",
        "runtimeCacheInputs": [
          "echo $NODE_ENV",
          "echo $NX_GTM",
          "echo $NX_INTERCOM",
          "echo $NX_LOGROCKET",
          "echo $NX_OAUTH_AUDIENCE",
          "echo $NX_OAUTH_DOMAIN",
          "echo $NX_OAUTH_KEY",
          "echo $NX_POSTHOG",
          "echo $NX_POSTHOG_APIHOST",
          "echo $NX_QOVERY_API",
          "echo $NX_URL"
