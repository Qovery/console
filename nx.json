{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "production",
        "^production",
        {
          "env": "NX_PUBLIC_GTM"
        },
        {
          "env": "NX_PUBLIC_INTERCOM"
        },
        {
          "env": "NX_PUBLIC_OAUTH_AUDIENCE"
        },
        {
          "env": "NX_PUBLIC_OAUTH_DOMAIN"
        },
        {
          "env": "NX_PUBLIC_OAUTH_KEY"
        },
        {
          "env": "NX_PUBLIC_POSTHOG"
        },
        {
          "env": "NX_PUBLIC_POSTHOG_APIHOST"
        },
        {
          "env": "NX_PUBLIC_QOVERY_API"
        },
        {
          "env": "NX_PUBLIC_QOVERY_WS"
        }
      ],
      "cache": true
    },
    "e2e": {
      "inputs": ["default", "^production"],
      "cache": true
    },
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json", "{workspaceRoot}/.eslintignore"],
      "cache": true
    },
    "build-storybook": {
      "inputs": ["default", "^production", "{projectRoot}/.storybook/**/*", "{projectRoot}/tsconfig.storybook.json"],
      "cache": true
    },
    "@nx/jest:jest": {
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json", "{workspaceRoot}/.eslintignore"],
      "cache": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": [
      "default",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/.storybook/**/*",
      "!{projectRoot}/tsconfig.storybook.json"
    ],
    "sharedGlobals": []
  },
  "generators": {
    "@nx/react": {
      "application": {
        "projectNameAndRootFormat": "derived",
        "style": "none",
        "linter": "eslint",
        "bundler": "webpack",
        "babel": true
      },
      "component": {
        "nameAndDirectoryFormat": "derived",
        "style": "none"
      },
      "hook": {
        "nameAndDirectoryFormat": "derived"
      },
      "library": {
        "projectNameAndRootFormat": "derived",
        "style": "none",
        "linter": "eslint",
        "unitTestRunner": "jest",
        "bundler": "none"
      }
    },
    "@nx/js": {
      "application": {
        "projectNameAndRootFormat": "derived"
      },
      "library": {
        "style": "none",
        "linter": "eslint",
        "unitTestRunner": "jest",
        "bundler": "none",
        "projectNameAndRootFormat": "derived"
      }
    }
  },
  "nxCloudAccessToken": "MzQ5ZWRiNjctYmE2NS00MmYyLTliZDktOWU5MWQ0MGYxNzY4fHJlYWQ=",
  "useInferencePlugins": false,
  "defaultBase": "staging",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      },
      "include": [
        "apps/console/**/*",
        "apps/console-e2e/**/*",
        "apps/design-system-e2e/**/*",
        "libs/domains/cloud-providers/data-access/**/*",
        "libs/domains/cloud-providers/feature/**/*",
        "libs/domains/clusters/data-access/**/*",
        "libs/domains/clusters/feature/**/*",
        "libs/domains/custom-domains/data-access/**/*",
        "libs/domains/custom-domains/feature/**/*",
        "libs/domains/environments/data-access/**/*",
        "libs/domains/environments/feature/**/*",
        "libs/domains/event/**/*",
        "libs/domains/organizations/data-access/**/*",
        "libs/domains/organizations/feature/**/*",
        "libs/domains/projects/data-access/**/*",
        "libs/domains/projects/feature/**/*",
        "libs/domains/service-helm/data-access/**/*",
        "libs/domains/service-helm/feature/**/*",
        "libs/domains/services/data-access/**/*",
        "libs/domains/services/feature/**/*",
        "libs/domains/users-sign-up/data-access/**/*",
        "libs/domains/users-sign-up/feature/**/*",
        "libs/domains/variables/data-access/**/*",
        "libs/domains/variables/feature/**/*",
        "libs/domains/variables/util/**/*",
        "libs/pages/application/**/*",
        "libs/pages/cluster/**/*",
        "libs/pages/clusters/**/*",
        "libs/pages/database/**/*",
        "libs/pages/environments/**/*",
        "libs/pages/events/**/*",
        "libs/pages/layout/**/*",
        "libs/pages/login/**/*",
        "libs/pages/logs/environment/**/*",
        "libs/pages/logs/infra/**/*",
        "libs/pages/onboarding/**/*",
        "libs/pages/overview/feature/**/*",
        "libs/pages/overview/ui/**/*",
        "libs/pages/services/**/*",
        "libs/pages/settings/**/*",
        "libs/pages/user/**/*",
        "libs/shared/assistant/feature/**/*",
        "libs/shared/auth/**/*",
        "libs/shared/console-shared/**/*",
        "libs/shared/enums/**/*",
        "libs/shared/factories/**/*",
        "libs/shared/iam/data-access/**/*",
        "libs/shared/iam/feature/**/*",
        "libs/shared/interfaces/**/*",
        "libs/shared/router/**/*",
        "libs/shared/routes/**/*",
        "libs/shared/toast/**/*",
        "libs/shared/util-const/**/*",
        "libs/shared/util-dates/**/*",
        "libs/shared/util-git/**/*",
        "libs/shared/util-hooks/**/*",
        "libs/shared/util-js/**/*",
        "libs/shared/util-node-env/**/*",
        "libs/shared/util-services/**/*",
        "libs/shared/util-tests/**/*",
        "libs/shared/util-types/**/*",
        "libs/shared/util-web-sockets/**/*",
        "libs/shared/utils/**/*",
        "libs/state/util-queries/**/*"
      ]
    },
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "previewTargetName": "preview",
        "serveStaticTargetName": "serve-static",
        "serveTargetName": "serve"
      }
    }
  ]
}
